{
    "name": "<%= _.slugify(appname) %>",
    "version": "0.0.0",
    "dependencies": {
            "jquery": "~1.10.2",
            "jasmine-jquery":"*",
            "underscore": "*",<% if (compassBootstrap) { %>
            "sass-bootstrap": "~3.0.0",<% } if (includeRequireJS) { %>
            "requirejs": "~2.1.8", <% } if (includeModernizr) { %>
            "modernizr": "~2.6.2",<% } if (angular) { %>
            "angular": "~1.2.0-rc.3",
            "angular-route":"~1.2.0-rc.3",
            "angular-mocks":"~1.2.0-rc.3",<% if(resourceModule) { %>
            "angular-resource": "~1.2.0-rc.3",<% } if(cookiesModule) { %>
            "angular-cookies": "~1.2.0-rc.3",<% } if(sanitizeModule) { %>
            "angular-sanitize": "~1.2.0-rc.3",<% } } else { %>
            "handlebars": "*", <% } %>
            "json3": "~3.2.4",
            "es5-shim": "~2.0.8"
    },
    "devDependencies": {<% if (angular) { %>
            "angular-mocks": "~1.2.0-rc.3",
            "angular-scenario": "~1.2.0-rc.3"<% } %>
    }
}
