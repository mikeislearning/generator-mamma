{
    "name": "<%= _.slugify(appname) %>",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "start": "node app.js"
    },
    "engines": {
        "node": ">=0.8.0"
    },
    "dependencies": {
        "express": "3.4.0",
        "mongoose": "3.6.15"
    },
    "devDependencies": {
        "grunt": "~0.4.1",
        "grunt-autoprefixer": "~0.2.0",
        "grunt-contrib-copy": "~0.4.1",
